<div class="panel panel-default" style="visibility: visible; opacity: 1; display: block; transform: translateY(0px);">
    <div class="panel-heading">
      <h2>Edit</h2>
    </div>
  <div class="panel-body row">
    <div class="col-lg-6">
      <%= form_for [@request, @influencer] do |f| %>
        <div class="field">
          <%= f.text_field :first_name, placeholder: 'First Name', class: 'form-control' %>
        </div>
        <br />
        <div class="field">
          <%= f.text_field :last_name, placeholder: 'Last Name', class: 'form-control' %>
        </div>
        <br />
        <div class="field">
          <%= f.text_field :email, placeholder: 'Email', class: 'form-control' %>
        </div>
        <br />
        <div class="field">
          <%= f.text_field :location, placeholder: 'Location', class: 'form-control input-normal' %>
        </div>
        <br />
        <div class="field">
          <%= f.text_field :facebook_url, placeholder: 'Facebook Url', class: 'form-control input-normal' %>
        </div>
        <br/>

        <div class="field">
          <%= f.text_field :twitter_url, placeholder: 'Twitter Url', class: 'form-control input-normal' %>
        </div>
        <br/>

        <div class="field">
          <%= f.text_field :linkedin_url, placeholder: 'LinkedIn Url', class: 'form-control input-normal' %>
        </div>
        <br/>

      
        <%= f.submit "Edit Influencer", class: "btn btn-success btn-block" %>
      <% end %>
    </div>
  </div>
</div> 